<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/keypad"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/man5_0"
    android:stretchColumns="*"
    >
    <TableRow >

        <Button
            android:id="@+id/keypad_1"
            android:text="1"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_2"
            android:text="2"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_3"
            android:text="3"
            style="@style/number_button"/>
    </TableRow>

    <TableRow >

        <Button
            android:id="@+id/keypad_4"
            android:text="4"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_5"
            android:text="5"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_6"
            android:text="6"
            style="@style/number_button"/>
    </TableRow>

    <TableRow >

        <Button
            android:id="@+id/keypad_7"
            android:text="7"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_8"
            android:text="8"
            style="@style/number_button"/>

        <Button
            android:id="@+id/keypad_9"
            android:text="9"
            style="@style/number_button"/>
    </TableRow>

</TableLayout>

